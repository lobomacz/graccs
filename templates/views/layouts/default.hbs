                        //get_municipalities(parent_id, area_id);
                    }
                    else  if (areaType === 3) {
                        get_municipality(parent_id, area_id);
                    }
                }
				
				function get_graph_by_area_values(point_year) {
					var frequency_values = [];
					
                    switch (frequency_type) {
                        case 'monthly':
                            frequency_values.push({ id: '01', text: 'Enero' });
                            frequency_values.push({ id: '02', text: 'Febrero' });
                            frequency_values.push({ id: '03', text: 'Marzo' });
                            frequency_values.push({ id: '04', text: 'Abril' });
                            frequency_values.push({ id: '05', text: 'Mayo' });
                            frequency_values.push({ id: '06', text: 'Junio' });
                            frequency_values.push({ id: '07', text: 'Julio' });
                            frequency_values.push({ id: '08', text: 'Agosto' });
                            frequency_values.push({ id: '09', text: 'Septiembre' });
                            frequency_values.push({ id: '10', text: 'Octubre' });
                            frequency_values.push({ id: '11', text: 'Noviembre' });
                            frequency_values.push({ id: '12', text: 'Diciembre' });
                            break;
                        case 'quarterly':
                            frequency_values.push({ id: '1', text: 'I Trimestre' });
                            frequency_values.push({ id: '2', text: 'II Trimestre' });
                            frequency_values.push({ id: '3', text: 'III Trimestre' });
                            frequency_values.push({ id: '4', text: 'IV Trimestre' });
                            break;
                        case 'biannual':
                            frequency_values.push({ id: '1', text: 'I Semestre' });
                            frequency_values.push({ id: '2', text: 'II Semestre' });
                            break;
                    }

					if (frequency_values && frequency_values.length > 0) {
                        $frequency.html('').select2({
                            theme: "bootstrap",
                            placeholder: "-- Seleccionar --",
                            allowClear: true,
                            data: frequency_values
                        });
					}
					else  {
                        $frequency.val(point_year).trigger("change");
                        $('#frequency').attr('disabled', 'disabled');
                    }

                    $area_types.val(minAreaToApply).trigger("change");
                }

                function get_municipalities(parent, municipality) {
                    $municipalities.html('').select2({
